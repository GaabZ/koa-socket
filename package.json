    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "koa": "^2.0.0-alpha.3",
    "minimist": "^1.2.0",
    "socket.io-client": "^1.3.7",
    "tape": "^4.2.2"
  }
